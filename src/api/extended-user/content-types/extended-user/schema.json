{
  "kind": "collectionType",
  "collectionName": "extended_users",
  "info": {
    "singularName": "extended-user",
    "pluralName": "extended-users",
    "displayName": "extendedUser",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "places": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::place.place",
      "mappedBy": "user"
    },
    "profilePicture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "age": {
      "type": "integer",
      "required": true,
      "min": 18
    },
    "favorites": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::favorite.favorite",
      "mappedBy": "user"
    }
  }
}
